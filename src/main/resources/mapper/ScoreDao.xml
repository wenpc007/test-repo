<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="ScoreDao">

    <update id="updateUserScore" parameterType="map">
        update lab_user
        set updateTime = now()
        ,score = ${score}
        where id = ${userId}
    </update>


</mapper>

